{
    "compilerOptions": {
      "target": "es2019",
      "module": "esnext",
      "lib": ["dom", "es2019"],
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": ["src","./typings.d.ts"]
  }